template header
subclass
magicMin
techIdeal
darkIdeal

package class

global ArrayList<Subclass> candidates;

template "campaign"

rule "Campaign Details Filter_@{row.rowNumber}"
no-loop true
when
    $cd: CampaignDetailsModel(magic>@{magicMin}, abs(tech-@{techIdeal})<4, abs(dark-@{darkIdeal})<5)
then
    candidates.remove(@{charClass})
end

end template
